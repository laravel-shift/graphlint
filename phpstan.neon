includes:
    - ./vendor/worksome/coding-style/phpstan.neon

parameters:
    paths:
        - src
        - tests

    level: 9

    ignoreErrors:
        - '#Call to an undefined method Pest\\Expectation.*#'
        -
            message: "#Undefined variable: \\$this#"
            path: tests/Pest.php
        -
            message: "#Cannot access property .* on mixed.#"
            path: tests/Pest.php